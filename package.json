{
  "name": "@nomadpixelgeek/squid-cache",
  "version": "1.0.2",
  "description": "Shared local filestore cache + unified replay CLI for Subsquid-based projects.",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "squid-cache-replay": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "subsquid",
    "cache",
    "replay",
    "squid",
    "evm",
    "analytics",
    "indexer"
  ],
  "author": "NomadPixelGeek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nomadpixelgeek/squid-cache"
  },
  "bugs": {
    "url": "https://github.com/nomadpixelgeek/squid-cache/issues"
  },
  "homepage": "https://github.com/nomadpixelgeek/squid-cache#readme",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
